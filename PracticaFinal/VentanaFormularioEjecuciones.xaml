<Window x:Class="PracticaFinal.VentanaFormularioEjecuciones"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaFinal"
        mc:Ignorable="d"
        Height="270" Width="300">
    <Grid Margin="10">
        <StackPanel>
            <Label>Repeticiones:</Label>
            <TextBox x:Name="RepeticionesTextBox" />
            <Label>Peso:</Label>
            <TextBox x:Name="PesoTextBox" />
            <Label>Fecha:</Label>
            <DatePicker x:Name="FechaPicker" />
            <Label>Hora (HH:mm):</Label>
            <TextBox x:Name="HoraTextBox" />
            
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5">
                <Button Content="Aceptar" Width="75" Margin="5" Click="Aceptar_Click" />
                <Button Content="Cancelar" Width="75" Margin="5" Click="Cancelar_Click" />
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
